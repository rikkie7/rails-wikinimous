<div class="d-flex justify-content-between align-items-center">
  <h1>Wikiminous</h1>
  <%=button_to "Add article", new_article_path, method: :get, class: "btn btn-primary"%>
</div>
<div class="container">
<%@articles.each do |article|%>
  <h3><%=link_to article.title, article_path(article)%></h3>
  <p><%=article.content[0..50]%>...</p>
<%end%>
</div>
